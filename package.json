{
  "name": "vue-cli3-template",
  "version": "0.1.0",
  "description": "A Vue Project",
  "scripts": {
    "serve": "vue-cli-service serve",
    "dll": "webpack --config webpack.dll.config.js --progress --color",
    "build": "vue-cli-service build",
    "lint": "eslint --fix --ext .js,.vue src",
    "lint:style": "stylelint src/assets/css/*.{scss,css} --fix --syntax scss"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run lint:style && git add ."
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.4.3",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "^4.2.3",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "sass-loader": "^8.0.0",
    "stylelint": "^12.0.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.13.0",
    "stylelint-webpack-plugin": "^1.1.2",
    "svg-sprite-loader": "^4.1.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.5"
  }
}
